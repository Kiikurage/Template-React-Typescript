{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "target": "ES6",
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true
  }
}
